</template>

<script>
import { go } from '../../libs/router'
  import {ViewBox,Headerbar,Search,Tab, TabItem, Popup, Group, Switch, Cell, Scroller, Tabbar, TabbarItem,Actionsheet,Spinner,Sticky,Swiper,SwiperItem,Dialog} from '../../components'
  var lastMoveTime = 0;
  var lastMoveStart = 0;
  var stopInertiaMove = false; // 是否停止缓动
  let tabEleList = []

  export default {
    components: {
      // 可以以key-value的形式注册组件, 此时挂载点的名字就是key
      // 否则挂载点和组件名字一致, 即vhead
      ViewBox,
      Headerbar,
      Search,

      Tab,
      TabItem,
      Popup,
      Group,
      Switch,
      Cell,
      Scroller,
      Tabbar,
      TabbarItem,
      Actionsheet,

      Spinner,
      Sticky,
      Swiper,
      SwiperItem,
      Dialog
    },
    data (){
      return {
        //屏幕宽度
        clientWidth:375,

        //数据
        mainData:{},
        dateData:[],
        saleData:{},
        pleasedData:{},
        calendarData:[],
        flightData:[],
        hotelData:[],
        RecommendData:[],

        showOnLineService:api.showOnLineService,

        //优惠信息个数
        preferentialNum:"",
        preferentialTitle:"",
        haspreferential:true,
        minPrice:'',
        moredate:true,

        //出发地
        DepartCityText:'',
        MoreDeparture:true, //是否显示更多出发地

        //分享
        apptitle :'分享标题',
        appdesc:'分享描述',
        applink:'跳转地址',
        appimgUrl:'图片地址',
        jsonParam :[],

        //滚动导航
        tabList:[],
        isTop:false,
        isHideHeader:false,

        //展开收起
        isOpenMoreFeature:true,

        showPage:false,
        showState:true,
        tabnowtxt:'产品特色',
        showask: false,
        showNoScroll:false, //起价说明

        //客户点评
        pleasedData:[],
        pleasedText:"整体满意度",
        //门店链接
        mendianurl:"http://mmy.aoyou.com/s/service/stores.html?forward="+location.href,
        //产品是否售罄
        issellout:false,
        qijiashuoming:'',
        isOpacity:0  //顶部导航栏透明度
      }
    },
    methods: {
      takeData:function(){
            g_selected_data={}
            var ProductID = this.$route.query.pid||''
            var tempthis= this
            var productParam={"ProductID":ProductID}
            g_selected_data.pid = ProductID

           //主信息
           api.post({
                path: api.config.microServiceHost.groupurl,
                controller: 'ProductForWap',
                action: 'Select_GroupProduct_ByProductID',
                param: JSON.stringify(productParam),
                useCache: false,
                useProxy: true,
                callback: function(obj) {
                    if(obj.ReturnCode==0){
                        tempthis.mainData = obj.Data
                        if(obj.Data){
                            tempthis.DepartCityText = tempthis.mainData.DepartCityText +'出发'
                        }
                        if(tempthis.mainData.ProductStatus !=30){
                            tempthis.issellout = true
                        }
                        g_mainData = tempthis.mainData
                        g_selected_data.an = tempthis.mainData.InterFlag
                        //推荐行程
                        var recommandParam={
                                "productType":tempthis.mainData.ProductType,
                                "departCity":tempthis.mainData.DepartCity,
                                "destinationCityText":tempthis.mainData.DestinationCityText,
                                "Count":3,
                                "productId":parseInt(ProductID)
                           }
                          api.post({
                                path: api.config.microServiceHost.groupurl,
                                controller: 'ProductForWap',
                                action: 'GetRecommendProducts',
                                param: JSON.stringify(recommandParam),
                                useCache: false,
                                useProxy: true,
                                callback: function(obj) {
                                    if(obj.ReturnCode==0){
                                        tempthis.RecommendData = obj.Data
                                        for(let pp=0,qq=tempthis.RecommendData.length;pp<qq;pp++){
                                            let strlist = ""
                                            for(let bb=0,dd= tempthis.RecommendData[pp].DepartDateList.length;bb<dd && bb<2;bb++){
                                                var temp = tempthis.RecommendData[pp].DepartDateList[bb]
                                                strlist += ","+ temp.substr(5,5)
                                            }
                                            if(tempthis.RecommendData[pp].DepartDateList.length>2)
                                            {
                                                strlist+="..."
                                            }
                                            tempthis.RecommendData[pp].DepartDateListStr = "日期"+strlist.substr(1)
                                        }

                                    }
                                }
                          })

                          //优惠信息列表
                          api.post({
                                path: api.config.microServiceHost.groupurl,
                                controller: 'ProductForWap',
                                action: 'Select_ProductPreferential_List__ByProductID',
                                param: JSON.stringify(productParam),
                                useCache: false,
                                useProxy: true,
                                callback: function(obj) {
                                    if(obj.ReturnCode==0){
                                        if(obj.Data && obj.Data.length>0){
                                            tempthis.preferentialNum = obj.Data.length
                                            tempthis.preferentialTitle = obj.Data[0].PreName
                                            var data = obj.Data
                                            for(var i in obj.Data){
                                                data[i].PreStartDate = changeDataForPre(obj.Data[i].PreStartDate)
                                                data[i].PreEndDate = changeDataForPre(obj.Data[i].PreEndDate)
                                            }

                                            g_specialoffers = tempthis.mainData.SpecialOffers
                                            if(tempthis.mainData.SpecialOffers!=null && tempthis.mainData.SpecialOffers.trim()!=""){
                                                //如果有更多优惠 增加一个优惠数量
                                                tempthis.preferentialNum++
                                            }
                                            g_preferentialData = data
                                        }else{
                                            if(tempthis.mainData.SpecialOffers!=null && tempthis.mainData.SpecialOffers.trim()!=""){
                                                tempthis.preferentialTitle = tempthis.mainData.SpecialOffers.trim()
                                                g_specialoffers = tempthis.mainData.SpecialOffers

                                            }else{
                                                tempthis.haspreferential = false
                                            }
                                        }
                                    }else{
                                        tempthis.haspreferential = false
                                    }

                                }
                          })

                          //更多出发地
                          var departureParam={
                                "departCityText":tempthis.DepartCityText.replace('出发',''),
                                "productId":ProductID
                           }
                          api.post({
                                path: api.config.microServiceHost.groupurl,
                                controller: 'ProductForWap',
                                action: 'GetProductTransportPolicy',
                                param: JSON.stringify(departureParam),
                                useCache: false,
                                useProxy: true,
                                callback: function(obj) {
                                    if(obj.ReturnCode==0){
                                        g_departure = obj.Data
                                    }else{
                                        tempthis.MoreDeparture = false
                                    }
                                }
                          })
                    }
                }
          })

          //价格日历信息列表
          api.post({
                path: api.config.microServiceHost.groupurl,
                controller: 'ProductForWap',
                action: 'GetProductPriceCalendar',
                param: JSON.stringify(productParam),
                useCache: false,
                useProxy: true,
                callback: function(obj) {
                    if(obj.ReturnCode==0){
                        if(!obj.Data || !obj.Data.PriceStockList || obj.Data.PriceStockList.length==0){
                            tempthis.issellout = true
                        }else{
                            if(obj.Data.PriceStockList.length<10){
                                tempthis.moredate = false
                            }
                            tempthis.minPrice = obj.Data.MinPrice
                            tempthis.dateData = []
                            for(var i in obj.Data.PriceStockList){
                                if(i<9){
                                    var tempdata = obj.Data.PriceStockList[i]
                                    tempdata.DepartDate = changeData(tempdata.DepartDate)
                                    tempthis.dateData.push(tempdata)
                                }
                            }
                            tempthis.dateData.ShowPriceDesc = obj.Data.ShowPriceDesc
                            tempthis.qijiashuoming = tempthis.dateData.ShowPriceDesc
                        }
                    }else{
                        tempthis.issellout = true
                    }
                    tempthis.changeDataNum()
                }
          })


          //客户满意度
          var recommdParam={
                "Count":1,
                "productId":ProductID
           }
            api.post({
                path: api.config.microServiceHost.groupurl,
                controller: 'productforwap',
                action: 'Select_StatisticsEvaluate_ByProductID',
                param: JSON.stringify(recommdParam),
                useCache: false,
                useProxy: true,
                callback: function(obj) {
                    if(obj.ReturnCode==0){
                        tempthis.pleasedData = obj.Data
                        if(tempthis.pleasedData.EvaluateTotal<=0){
                            tempthis.pleasedText = "暂无评价"
                        }
                    }else{
                        tempthis.pleasedData = {"GoodValue":0,"EvaluateTotal":0}
                    }
                }
          })
      },
      onLineService:function(){
        api.onlineService(this.mainData.MainTitle,this.mainData.ProductID,this.mainData.PictureUrl ,location.href)
      },
      telClick:function(tel){
          api.telClick(tel)
      },//客服热线
      openstartprice:function(){
        this.showNoScroll = true
      },
      openMoreFeature:function(){
          this.isOpenMoreFeature = !this.isOpenMoreFeature
          if(this.isOpenMoreFeature){
            $('#view_box_body').animate({ scrollTop: $("#Morefeaturep")[0].offsetTop}, 200);
          }
      },//展开或收起特色信息
      popupask:function(){
        this.showask = !this.showask
      },//页面底部咨询弹出
      toDate:function(item){
          //if(item.DepartDate&&item.DepartDate.indexOf('T')>=0)
          //{
              g_selected_data.depDate=item.DepartDate
              console.log(g_selected_data.depDate)
              this.$router.go('date')
          //}

      },
      scrollFunc:function(e){
        let eleScroll = document.getElementById('view_box_body').scrollTop
        if(eleScroll > 142){
            this.isOpacity = 1
            this.isHideHeader = true
        }else if(eleScroll < 20){
            this.isHideHeader = false
            this.isOpacity = 0
        }else {
            this.isOpacity = eleScroll/142
            this.isHideHeader = true
        }
        for(let p=0,q=tabEleList.length;p<q;p++){
            let thisele = tabEleList[p].offsetTop-40
            if(eleScroll >= thisele){
                this.tabnowtxt = $(tabEleList[p]).attr('headtext')
            }
        }
      },
        //标签切换 START
      tabSelect:function (i) {
        var num = tabEleList[i].offsetTop-40
        $('#view_box_body').animate({ scrollTop: num}, 200);
      },
      changeDataNum:function(){
        this.clientWidth = document.body.clientWidth
        if(this.clientWidth<360){
            this.dateData = this.dateData.slice(0,7)
        }
      },//设置屏幕宽度 并改变显示日期个数
      route:{
        data:function(transition){
            if(transition.to!=null&&transition.to!=undefined)
            {
                this.pid=this.$route.query.pid||''
                var date = new Date();
                var m=(date.getMonth() + 1)+''
                if(m.length==1)
                {
                    m='0'+m
                }
                var d=date.getDate()+''
                if(d.length==1)
                {
                    d='0'+d
                }
                this.depDate=date.getFullYear()+'-'+m+'-'+d
                this.takeData()
            }
        }
    },
        shareFun:function()
        {
            api.openShareDialog(this.mainData.MainTitle, this.mainData.SubTitle, window.location.href, this.mainData.PictureUrl, {})
        },//分享
        back:function(){
            api.pageBack()
        },//返回
        devilScrollFunc:function(type){
            let e = event
            if(type=='start'){
                //缓动代码
                lastMoveStart = e.currentTarget.scrollTop;
                lastMoveTime = e.timeStamp || Date.now();
                stopInertiaMove = true;
              }else if(type=='move'){
                //缓动代码
                var nowY = e.currentTarget.scrollTop;
                var nowTime = e.timeStamp || Date.now();
                stopInertiaMove = true;
                if(nowTime - lastMoveTime > 300) {
                    lastMoveTime = nowTime;
                    lastMoveStart = nowY;
                }
              }else{
                //缓动代码
                var nowY = e.currentTarget.scrollTop;
                if(lastMoveStart == nowY){
                  return;
                }
                var nowTime = e.timeStamp || Date.now();
                var v = (nowY - lastMoveStart) / (nowTime - lastMoveTime); //最后一段时间手指划动速度
                stopInertiaMove = false;
                var _this = this;
                (function(v, startTime) {
                    var dir = v > 0 ? -1 : 1; //加速度方向
                    var deceleration = dir*0.0006;
                    var duration = v / deceleration; // 速度消减至0所需时间
                    if(Math.abs(duration)>3000){
                      duration = 1000
                    }
                    setTimeout(function(){
                      _this.scrollFunc()
                      return;
                    }, Math.abs(duration)-500);
                })(v, nowTime);
              }
            this.scrollFunc()
        }
    },
    created(){
        this.takeData()
    },
    ready() {
      tabEleList = $('.tab-list');

    }

  }

function changeData(data){
    //2017-02-01T00:00:00 To 02/01
    data = data.substr(5,2) + '/' + data.substr(8,2)
    return data
}

function changeTime(val){
    //1125 To 11:25
    val = val.substr(0,2) + ":" + val.substr(2,2)
    return val
}

function changeDataForPre(data){
    //2016-03-21T00:00:00 To 2016年03月21日 00:00:00
    data = data.substr(0,4) + '年'+data.substr(5,2) + '月'+data.substr(8,2)+'日 ' + data.substr(11)
    return data
}

</script>
