<script>
import { go } from '../../libs/router'
  import {ViewBox,Headerbar,Search,Tab, TabItem, Popup, Group, Switch, Cell, Scroller, Tabbar, TabbarItem,Actionsheet,Spinner,Sticky,Swiper,SwiperItem,Dialog} from '../../components'
  var lastMoveTime = 0;
  var lastMoveStart = 0;
  var stopInertiaMove = false; // 是否停止缓动
  let tabEleList = []

import store from '../../vuex/store'/*0228 F*/
const commit = store.commit || store.dispatch /*0228 F*/

  export default {
        components: {
          // 可以以key-value的形式注册组件, 此时挂载点的名字就是key
          // 否则挂载点和组件名字一致, 即vhead
          ViewBox,
          Headerbar,
          Search,

          Tab,
          TabItem,
          Popup,
          Group,
          Switch,
          Cell,
          Scroller,
          Tabbar,
          TabbarItem,
          Actionsheet,

          Spinner,
          Sticky,
          Swiper,
          SwiperItem,
          Dialog
        },
        data (){
          return {
            //屏幕宽度
            clientWidth:375,
            ProductId:0,
            Interflag:0,
            ProductType:0,
            //数据
            mainData:{},
            dateData:[],
            saleData:{},
            pleasedData:{},
            calendarData:[],
            flightData:[],
            hotelData:[],
            lstHotelRoomInfoDTO:[],
            RecommendData:[],
            //优惠信息个数
            preferentialNum:0,
            haspreferential:true,
            preferentialTitle:"",

            //滚动导航
            tabList:[],
            isTop:false,
            isHideHeader:false,
            DepartCityText:"",

            //展开收起
            isOpenMoreFeature:true,

            showPage:false,
            showState:true,
            tabnowtxt:'产品特色',
            showask: false,
            showroombrief:false,
            ShowPrice:0,
            showNoScroll:false,
            ShowPriceDesc:"",
            MoreDeparture:true,
            showOnLineService:api.showOnLineService,
            showMoreDate:false,
            isShowPrice:false,
            //产品是否售罄
            issellout:false,
            statisticsEvaluate:{
                RecommendValue: 0,
                EvaluateTotal: 0,
                getTotalStr:function(){
                    return "客户点评（"+this.EvaluateTotal+"）"
                }
            },
            showShare:api.showShare,
            isShowOhinfo:false,
            isOpacity:0  //顶部导航栏透明度
            // appTitle :'分享标题',
            // appContent:'分享描述',
            // applinkUrl:'跳转地址',
            // appimgUrl:'图片地址',
            // jsonParam :[]
          }
        },
        methods: {
          takeData:function(){
              //获取数据
                var vm = this
                var useCache = false
                var useProxy = true
                p_selected_data={}
                p_selected_data.pid=this.$route.query.pid||api.getQueryString("pid")||""
                vm.ProductId=p_selected_data.pid
                var pgParam = {
                    ProductID:vm.ProductId,
                }

                api.post({
                path: api.config.microServiceHost.packageurl,
                controller: 'package',
                action: 'Select_PackageProduct_ByProductID',
                param: JSON.stringify(pgParam),
                useCache: useCache,
                useProxy: useProxy,
                callback: function(obj) {
                    if (obj.ReturnCode == 0) {
                        var data = obj.Data;
                        vm.mainData=data
                        p_mainData = vm.mainData
                        if(obj.Data.ProductStatus!=30)
                        {
                            vm.issellout=true
                        }
                        vm.DepartCityText=vm.mainData.DepartCityText+"出发"
                        vm.flightData = data.AirProductViewList
                        vm.hotelData = data.HotelProductViewList
                        g_hotel_list=data.HotelProductViewList
                        p_selected_data.confirmType=data.ConfirmType
                        p_selected_data.i=data.InterFlag
                        p_selected_data.pt=data.ProductType
                        vm.ProductType=data.ProductType
                        if(data.OtherInfo!=null&&data.OtherInfo!='')
                        {
                            vm.isShowOhinfo=true
                        }
                        api.initShareDialog(vm.mainData.MainTitle, '我在遨游网发现好品质旅游，好旅游上遨游', window.location.href, vm.mainData.PictureUrl)

                        //获取出发地信息
                        var dpParam = {
                            ProductID:vm.ProductId,
                            DepartCityText:vm.DepartCityText.replace('出发','')
                        }
                        api.post({
                            path: api.config.microServiceHost.packageurl,
                            controller: 'Common',
                            action: 'GetProductTransportPolicyForWap',
                            param: JSON.stringify(dpParam),
                            useCache: useCache,
                            useProxy: useProxy,
                            callback: function(obj) {
                                if (obj.ReturnCode == 0) {
                                    p_departure = obj.Data
                                }
                                else
                                {
                                    vm.MoreDeparture=false
                                }
                            }
                        });
                        //获取推荐产品
                        var rmParam = {
                           ProductType:data.ProductType,
                           DepartCity:data.DepartCity,
                           DestinationCityText:data.DestCityName,
                           TakeNum:3,
                           ProductID:vm.ProductId
                        }
                        api.post({
                            path: api.config.microServiceHost.packageurl,
                            controller: 'Common',
                            action: 'Select_RecommendProduct_List_ByProductID',
                            param: JSON.stringify(rmParam),
                            useCache: useCache,
                            useProxy: useProxy,
                            callback: function(obj) {
                                if (obj.ReturnCode == 0) {
                                    vm.RecommendData = obj.Data
                                    for(let pp=0,qq=vm.RecommendData.length;pp<qq;pp++){
                                        let strlist = ""
                                        for(let bb=0,dd= vm.RecommendData[pp].DepartDateList.length;bb<dd&&bb<2;bb++){
                                            var temp = vm.RecommendData[pp].DepartDateList[bb]
                                            strlist += ","+ temp.substr(5,5)
                                        }
                                        if(vm.RecommendData[pp].DepartDateList.length>2)
                                        {
                                            strlist+="..."
                                        }
                                        vm.RecommendData[pp].DepartDateListStr = "日期"+strlist.substr(1)
                                    }
                                }
                                vm.setScrollTop()
                            }
                        });

                        commit('UPDATE_LOADING',false)/*0228 F*/
                    }
                }
            });
            this.changeDataNum()
          },
           toDate:function(item){
              if(item&&item.DepartDate&&item.DepartDate.indexOf('T')>=0)
              {
                  p_selected_data.depDate=item.DepartDate.split('T')[0]
                  this.$router.go('date')
              }
              else
              {
                   this.$router.go('date')
              }
          },
          //获取价格相关
          getPriceInfo:function(){
            var vm = this
                var useCache = false
                var useProxy = true
                var pgParam = {
                    ProductID:vm.ProductId,
                }
                api.post({
                    path: api.config.microServiceHost.packageurl,
                    controller: 'package',
                    action: 'Select_PackageProductDayPriceList_ByProductID',
                    param: JSON.stringify(pgParam),
                    useCache: useCache,
                    useProxy: useProxy,
                    callback: function(obj) {
                        if (obj.ReturnCode == 0) {
                            var data = obj.Data
                           if(data.DayPriceList!=null)
                            {
                                if(data.DayPriceList.length==0)
                                {
                                    vm.issellout=true
                                }
                                vm.isShowPrice=data.DayPriceList.length>0
                                vm.ShowPrice=data.ShowPrice
                                vm.showMoreDate=data.DayPriceList.length>8
                                vm.ShowPriceDesc=data.ShowPriceDesc
                                if(data.DayPriceList.length>9)
                                {
                                    vm.dateData=data.DayPriceList.splice(0,9)
                                }
                                else
                                {
                                    vm.dateData=data.DayPriceList
                                }
                            }
                            else{
                                vm.issellout=true
                            }
                            vm.changeDataNum()
                        }
                    }
                });
          },
          //获取优惠信息
          getPreferenceInfo:function(){
            //获取数据
              var vm = this
                var useCache = false
                var useProxy = true
                var pgParam = {
                    ProductID:vm.ProductId
                }
                api.post({
                    path: api.config.microServiceHost.packageurl,
                    controller: 'Common',
                    action: 'Select_ProductPreferential_List_ByProductID',
                    param: JSON.stringify(pgParam),
                    useCache: useCache,
                    useProxy: useProxy,
                    callback: function(obj) {
                        if (obj.ReturnCode == 0) {
                            if(obj.Data && obj.Data.length>0){
                                vm.preferentialNum = obj.Data.length+"个优惠"
                                vm.preferentialTitle = obj.Data[0].PreName
                            }else{
                                if(vm.SpecialOffers!=null && vm.SpecialOffers.trim()!=""){
                                    vm.preferentialTitle = vm.SpecialOffers.trim()
                                }else{
                                    vm.haspreferential = false
                                }
                            }
                        }
                        else
                        {
                            vm.haspreferential = false
                        }
                    }
                });

          },
          //产品评论
          getCommentInfo:function()
          {
            var vm = this
            var useCache = false
            var useProxy = true
            var pgParam = {
                ProductID:vm.ProductId
            }
            api.post({
                path: api.config.microServiceHost.packageurl,
                controller: 'Common',
                action: 'Select_StatisticsEvaluate_ByProductID',
                param: JSON.stringify(pgParam),
                useCache: useCache,
                useProxy: useProxy,
                callback: function(obj) {
                    if (obj.ReturnCode == 0) {
                        var data=obj.Data
                        vm.statisticsEvaluate.RecommendValue=data.RecommendValue
                        vm.statisticsEvaluate.GoodValue=data.GoodValue
                        vm.statisticsEvaluate.EvaluateTotal=data.EvaluateTotal
                        p_statistics_evaluate=vm.statisticsEvaluate
                    }
                }
            });
          },
          setScrollTop() {
            if(!localStorage.getItem('top')){
               //
            }else{
                let thisvm = this
                setTimeout(() => {
                    document.querySelector('#view_box_body').scrollTop = localStorage.getItem('top')
                    localStorage.setItem('top',  0)
                    thisvm.scrollFunc()
                }, 650)
