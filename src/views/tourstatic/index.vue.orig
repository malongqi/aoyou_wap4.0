                    .changecolor2(10);
                    .changecolor2(@n, @i: 6) when (@i =< @n) {
                        li:nth-child(@{i}) {
                            i{background: #FF701B;}
                        }
                        .changecolor2(@n, (@i + 1));
                    }

                    .changecolor3(15);
                    .changecolor3(@n, @i: 11) when (@i =< @n) {
                        li:nth-child(@{i}) {
                            i{background: #51AE63;}
                        }
                        .changecolor3(@n, (@i + 1));
                    }

                    .changecolor4(20);
                    .changecolor4(@n, @i: 16) when (@i =< @n) {
                        li:nth-child(@{i}) {
                            i{background: #47B9B1;}
                        }
                        .changecolor4(@n, (@i + 1));
                    }

                    .changecolor5(25);
                    .changecolor5(@n, @i: 21) when (@i =< @n) {
                        li:nth-child(@{i}) {
                            i{background: #2E97D2;}
                        }
                        .changecolor5(@n, (@i + 1));
                    }

                    .changecolor6(30);
                    .changecolor6(@n, @i: 26) when (@i =< @n) {
                        li:nth-child(@{i}) {
                            i{background: #9F3691;}
                        }
                        .changecolor6(@n, (@i + 1));
                    }
                }
            }

