                    let upperVal = String(val).toUpperCase();
                    for (let i = 0, j = this.cityJson.length; i < j; i++) {
                        let _temstr = this.cityJson[i]['CityName']
                        if (String(this.cityJson[i]['CityNamePY']).toUpperCase().startsWith(upperVal)) {
