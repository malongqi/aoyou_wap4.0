              api.toSaleStores()
           },//去门店

            //打电话 /*0218 F*/
            tel(){
                window.location.href='tel:'+ 4006006666 /*0218 F*/
            },

            //展示立即预订页 点击开始预订按钮
            showBookList(){
                this.isBookShow = true
                for(var i=0;i<this.visaPackageList.length;i++){
                    this.visaPackageList[i].SelectType = false
                }
            },

            //关闭立即预订页 点击立即预订头部关闭按钮
            closeBookList(){
                this.isBookShow = false
                for(var i=0;i<this.visaPackageList.length;i++){
                    this.visaPackageList[i].SelectType = false
                }
            },


