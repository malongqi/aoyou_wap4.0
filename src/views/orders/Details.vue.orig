                    controller: 'OrderData',
                    action: 'ToPay',
                    param: JSON.stringify(paypara),
                    useCache: useCache,
                    useProxy: useProxy,
                    callback: function(obj) {
                        if (obj.ReturnCode == 0) {

                            if (true) {
                                var successParam = {
                                    OrderID: orderID,
                                    MainOrderID: mainorderid,
                                    OrderNo: orderNo,
                                    IsCanPay: 1,
                                    PayID: obj.Data,
                                    ProductType: producttype,
                                    SubProductType: subproducttype
                                };

                                if (window.webkit != null && window.webkit != undefined) {
                                    window.webkit.messageHandlers.bookingSuccess.postMessage(JSON.stringify(successParam));
                                } else if (window.action != null && window.action != undefined) {
                                    window.action.payment(JSON.stringify(successParam));
                                } else {
                                    var payhref;
                                    
                                    if (orderType==1||orderType == 27) {
                                        //机票
                                        payhref = "https://mpay.aoyou.com/s/flightpay/index.html?orderid=" + successParam.OrderID +
                                            "&mainorderid=" + successParam.MainOrderID +
                                            "&payid=" + successParam.PayID;

                                    } else {
                                        payhref = "https://mpay.aoyou.com/S/Pay/Index.html?orderid=" + successParam.OrderID +
                                            "&mainorderid=" + successParam.MainOrderID +
                                            "&payid=" + successParam.PayID;

                                    }
                                    window.location.href = payhref;
                                }
                            }
                        }
                    }
                });
            },
            CancelOrder(cancelorderinfo, couponParam) {
                api.post({
                    path: api.config.microServiceHost.orderurl,
                    controller: 'OrderData',
                    action: 'CancelOrder',
                    param: JSON.stringify(cancelorderinfo),
                    useCache: useCache,
                    useProxy: useProxy,
                    callback: function(obj) {
                        if (obj.ReturnCode == 0) {
                            globalThis.showCancelToast = true;
                        }
                    }
                });
                api.post({
                    path: api.config.microServiceHost.preferentialmicro,
                    controller: 'Bargain',
                    action: 'GetCoupons',
                    param: JSON.stringify(couponParam),
                    useCache: useCache,
                    useProxy: useProxy,
                    callback: function(obj) {}
                });
            },
            //获取机票赠送优惠
            GetAirPreferential(SubOrderId, orderNo) {
                var p = {
                    "OrderNumber": orderNo,
                    "SubOrderId": SubOrderId
                };

                api.post({
                    path: api.config.microServiceHost.preferentialmicro,
                    controller: 'Preferential',
                    action: 'SelectHasUsedSchemeByOrderNumber',
                    param: JSON.stringify(p),
                    useCache: useCache,
                    useProxy: useProxy,
                    callback: function(obj) {
                        if (obj.ReturnCode == 0) {
                            if (obj.Data) {
                                if (obj.Data.PolicyList) {
                                    var p = obj.Data.PolicyList;
                                    for (var i = 0; i < p.length; i++) {
                                        if (p[i].WayTypeIsOther == true) {
                                            globalThis.preferentials.push(p[i].Preferential_DisplayName);
                                        } else {
                                            globalThis.preferentialsother.push({
                                                "key": p[i].Preferential_DisplayName,
                                                "value": p[i].Preferential_Amount
                                            });
                                            globalThis.totalmoney-=p[i].Preferential_Amount;
                                        }
                                    }
                                }
                            }
                        }
                    }
                });
            },
            alerthide() {
                window.location.reload();
            },
            hideTerms() {
                this.isTermsShow = false
            },
            cancelcountdown(countdown) {
                var cd = countdown;
                if (countdowntime) {
                    clearInterval(countdowntime);
                }
                countdowntime = setInterval(function() {

                    var d = parseInt(cd / 86400);
                    var h = parseInt((cd % 86400) / 3600);
                    var m = parseInt((cd % 86400 % 3600) / 60);
                    var s = cd % 86400 % 3600 % 60;

                    var txt = "";
                    if (d > 0) {
                        txt += d + "天";
                    }
                    if (h > 0) {
                        txt += h + "时";
                    }

                    txt += m + "分";
                    txt += s + "秒";
                    if (m == 0 && s >= 0) {
                        txt = "最后一分钟";
                    }
                    globalThis.countdowntxt = txt;
                    cd--;
                    if (cd == 0) {
                        clearInterval(countdowntime);
                        globalThis.showcountdowntxt = false;
                    }
                }, 1000);
            }
        },

        created() {
            // this.axios.get("/static/booking.json").then((response) => {
            //     this.tmpList = response.data
            //     this.flightList = response.data.FlightList
            // })
        },

        ready() {
            // this.axios.get("/static/order.json").then((response) => {
            //   this.lists = response.data.FreeDetails
            // })
            globalThis = this;
            var memberid = api.getCookie("memberid");
            if (memberid == undefined || memberid == "") {
                memberid = this.$route.query.memberid;
            }
            if (memberid == undefined || memberid == "") {
                var url = encodeURIComponent(location.href)
                api.toLogin(url, url, "back")
                return false
                    //                window.location.href = "http://mpass.aoyou.com?from=" + encodeURIComponent(location.href) + "&forward=" + encodeURIComponent(location.href)
            }
            this.getOrderPara.memberid = memberid;
            var orderno = this.$route.query.orderno;
            var mainorderid = this.$route.query.mainorderid;
            var suborderid = this.$route.query.suborderid;
            var type = this.$route.query.type;

            GetOrderDeatail(this.getOrderPara.memberid, orderno, mainorderid, suborderid, type);
        }
    }

    var globalThis;
    var useCache = false;
    var useProxy = true;
    var countdowntime;

    function GetOrderDeatail(memberid, orderno, mainorderid, suborderid, type) {
        var useCache = false;
        var useProxy = true;
commit('UPDATE_LOADING',true)
        api.post({
            path: api.config.microServiceHost.orderurl,
            controller: 'OrderData',
            action: 'GetOrderDetail',
            param: JSON.stringify({
                "memberid": memberid,
                "mainorderid": mainorderid,
                "suborderid": suborderid,
                "orderno": orderno,
                "type": type
            }),
            useCache: useCache,
            useProxy: useProxy,
            callback: function(obj) {
commit('UPDATE_LOADING',false)

                if (obj.ReturnCode == 0) {
                    globalThis.lists = JSON.parse(obj.Data);
                    if (globalThis.lists.AirSegments != null) {
                        var allac=true;//全部都是国航订单
                        for (var i = 0; i < globalThis.lists.AirSegments.length; i++) {
                            var segitem=globalThis.lists.AirSegments[i];
                            segitem.No=i+1;
                            if (segitem.AirChannel == 1) {
                                globalThis.acairsegments.push(segitem);
                            } else {
                                allac=false;
                                globalThis.airsegments.push(segitem);
                            }

                           globalThis.totalmoney+=(segitem.UnitPrice+segitem.FuelFee+segitem.AirportFee)*globalThis.lists.TotalCount+(segitem.ChdUnitPrice+segitem.ChdFuelFee+segitem.ChdAirportTax)*globalThis.lists.ChdTotalCount;
                        }
                        globalThis.lists.allac=allac;
                    }
                    if (globalThis.lists.CancelCountDown > 0) {

                        globalThis.cancelcountdown(globalThis.lists.CancelCountDown);
                        globalThis.showcountdowntxt = true;
                    }
                    //退改规则

                    globalThis.showTerms = globalThis.lists.HaveSegmentRules == 1
                    globalThis.GetAirPreferential(globalThis.lists.SubOrderID, globalThis.lists.OrderNO);

                }
            }
        });
    }
</script>


